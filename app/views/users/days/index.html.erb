<p id="notice"><%= notice %></p>

<h1>Users Days</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users_days.each do |users_day| %>
      <tr>
        <td><%= link_to 'Show', users_day %></td>
        <td><%= link_to 'Edit', edit_users_day_path(users_day) %></td>
        <td><%= link_to 'Destroy', users_day, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Users Day', new_users_day_path %>
